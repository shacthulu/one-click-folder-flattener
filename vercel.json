{
  "version": 2,
  "name": "one-click-folder-flattener",
  "alias": ["folder-flattener.bitfire.dev"],
  "routes": [
    {
      "src": "/tools/one-click-folder-flattener/(.*)",
      "dest": "/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/tools/one-click-folder-flattener/(.*)",
      "destination": "/$1"
    }
  ]
}
